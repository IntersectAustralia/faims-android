			attributes = entity.getAttributes();
			for (attr : attributes) {
				if ("choice".equals(attr.getName())) {
					if (choices.get(choices.size()-1).get(0).equals(attr.getValue("vocab"))) {
						loadedValue = attr.getValue("freetext");
					}
				}
			}
		}
	});
}

