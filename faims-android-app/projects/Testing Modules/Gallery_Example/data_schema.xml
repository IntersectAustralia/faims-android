<?xml version="1.0" ?>
<?xml-stylesheet type="text/xsl" href="sampleDataXML.xsl"?>
<dataSchema name="LogicTest" preparer="Nobody">
    
  <RelationshipElement name="AboveBelow" type="hierarchy">
    <description>
      Indicates that one element is above or below another element.
    </description>
    <parent>
      Above
    </parent>
    <child>
      Below
    </child>
    <property type="string" name="relationship" isIdentifier="true">
      <bundle>DOI</bundle>
    </property>
    <property type="string" name="name">
      <bundle>DOI</bundle>
    </property>
  </RelationshipElement>
  
  <RelationshipElement name="NextTo" type="container">
    <description>
      Indicates that one element is next to another element.
    </description>
    <property type="string" name="relationship" isIdentifier="true">
      <bundle>DOI</bundle>
    </property>
    <property type="string" name="name">
      <bundle>DOI</bundle>
    </property>
  </RelationshipElement>
  
  <RelationshipElement name="Similar" type="bidirectional">
    <description>
      Indicates that one element similar to another element.
    </description>
    <property type="string" name="relationship" isIdentifier="true">
      <bundle>DOI</bundle>
    </property>
    <property type="string" name="name">
      <bundle>DOI</bundle>
    </property>
  </RelationshipElement>
  
  <ArchaeologicalElement name="Simple">
    <description>
      An simple entity
    </description>
    <property type="string" name="entity" isIdentifier="true">
      <bundle>DOI</bundle>
    </property>
    <property type="string" name="name">
      <bundle>DOI</bundle>
    </property>
    <property type="integer" name="value">
      <bundle>DOI</bundle>
    </property>
    <property type="timestamp" name="timestamp">
      <bundle>DOI</bundle>
    </property>
    <property type="dropdown" name="type">
	  <bundle>DOI</bundle>
	  <lookup>
	      <term>Type A</term>
	      <term>Type B</term>
	      <term>Type C</term>
	      <term>Type D</term>
	  </lookup>
    </property>
    <property type="dropdown" name="location">
      <bundle>DOI</bundle>
      <lookup>
        <term>Loc A</term>
        <term>Loc B</term>
        <term>Loc C</term>
        <term>Loc D</term>
      </lookup>
    </property>
