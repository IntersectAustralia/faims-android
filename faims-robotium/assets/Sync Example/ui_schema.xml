<h:html xmlns="http://www.w3.org/2002/xforms"
        xmlns:h="http://www.w3.org/1999/xhtml"
        xmlns:ev="http://www.w3.org/2001/xml-events"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:jr="http://openrosa.org/javarosa">
  <h:head>
    <h:title>Simple Example</h:title>

    <model>
      <instance>
        <faims id="simple_example">
          <style>
            <orientation>
                <orientation></orientation>
            </orientation>
            <even>
                <layout_weight></layout_weight>
            </even>
          </style>
          <user>
              <tab1>
                  <users></users>
              </tab1>
          </user>
          <menu>
              <tab1>
                  <options></options>
              </tab1>
          </menu>
          <tabgroup1>
              <tab1>
                  <name></name>
                  <value></value>
                  <timestamp></timestamp>
                  <location></location>
                  <type></type>
                  <gallery></gallery>
                  <rocks></rocks>
                  <files></files>
                  <attachfile></attachfile>
                  <pictures></pictures>
                  <attachpicture></attachpicture>
                  <videos></videos>
                  <attachvideo></attachvideo>
                  <audios></audios>
                  <attachaudio></attachaudio>
                  <viewattached></viewattached>
                  <container2>
                    <child1>
                      <save></save>
                    </child1>
                    <child2>
                      <clear></clear>
                    </child2>
                  </container2>
                  <delete></delete>
                  <close></close>
              </tab1>
              <tab2>
                  <entities></entities>
                  <load></load>
              </tab2>
          </tabgroup1>
          <tabgroup2>
              <tab1>
                  <name></name>
                  <value></value>
                  <timestamp></timestamp>
                  <location></location>
                  <type></type>
                  <gallery></gallery>
                  <rocks></rocks>
                  <files></files>
                  <attachfile></attachfile>
                  <pictures></pictures>
                  <attachpicture></attachpicture>
                  <videos></videos>
                  <attachvideo></attachvideo>
                  <audios></audios>
                  <attachaudio></attachaudio>
                  <viewattached></viewattached>
                  <container2>
                    <child1>
                      <save></save>
                    </child1>
                    <child2>
                      <clear></clear>
                    </child2>
                  </container2>
                  <delete></delete>
                  <close></close>
              </tab1>
              <tab2>
                  <relationships></relationships>
                  <load></load>
              </tab2>
          </tabgroup2>
          <tabgroup3>
              <tab1>
                  <start></start>
                  <stop></stop>
              </tab1>
          </tabgroup3>
          <tabgroup4>
              <tab1>
                  <entities></entities>
                  <relationships></relationships>
                  <verb></verb>
                  <add></add>
              </tab1>
          </tabgroup4>
        </faims>
      </instance>
      
      <bind nodeset="/faims/tabgroup1/tab1/value" type="decimal"/>
      <bind nodeset="/faims/tabgroup2/tab1/value" type="decimal"/>
      
    </model>
  </h:head>

  
  <h:body>
    <group ref="style">
      <label></label>
      <group ref="orientation">
        <label></label>
        <input ref="orientation">
            <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label></label>
        <input ref="layout_weight">
            <label>1</label>
        </input>
      </group>
    </group>
    <group ref="user">
      <label>Users List</label>
      <group ref="tab1" faims_scrollable="false">
        <label>User List</label>
        <select1 ref="users" appearance="compact">
          <label>Users:</label>
          <item>
            <label>dummy</label>
            <value>dummy</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="menu">
      <label>Options</label>
      <group ref="tab1" faims_scrollable="false">
        <label>Options</label>
        <select1 ref="options" appearance="compact">
          <label>Options:</label>
          <item>
            <label>dummy</label>
            <value>dummy</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="tabgroup1" faims_archent_type="small">
      <label>Entity</label>
      <group ref="tab1" faims_hidden="false">
        <label>Save Entity</label>
	        <input ref="name" faims_attribute_name="name" faims_attribute_type="freetext">
	          	<label>Name:</label>
	        </input>
	        <input ref="value" faims_attribute_name="value" faims_attribute_type="measure">
	            <label>Value:</label>
	        </input>
	        <input ref="timestamp" faims_attribute_name="timestamp" faims_attribute_type="freetext" faims_read_only="true" faims_certainty="false">
              <label>Timestamp:</label>
            </input>
            <select ref="location" faims_attribute_name="location" faims_attribute_type="vocab">
                <label>Location:</label>
                <item>
                	<label>dummy</label>
                	<value>dummy</value>
              	</item>
            </select>
	        <select1 ref="type" faims_attribute_name="type" faims_attribute_type="vocab">
	            <label>Type:</label>
	            <item>
	            	<label>dummy</label>
	            	<value>dummy</value>
	          	</item>
	        </select1>
	        <select1 ref="gallery" type="image" faims_attribute_name="gallery" faims_attribute_type="vocab">
	            <label>Gallery:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select1>
	        <select1 ref="rocks" type="image" faims_attribute_name="rocks" faims_attribute_type="vocab">
	            <label>Rocks:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select1>
	        <select ref="files" type="file" faims_attribute_name="file" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Files:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachfile">
	            <label>Attached File:</label>
	        </trigger>
	        <select ref="pictures" type="camera" faims_sync="true" faims_attribute_name="picture" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Pictures:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachpicture">
	            <label>Take picture</label>
	        </trigger>
	        <select ref="videos" type="video" faims_attribute_name="video" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Videos:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachvideo">
	            <label>Take video</label>
	        </trigger>
	        <select ref="audios" type="file" faims_attribute_name="audio" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Audios:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachaudio">
	            <label>Record Audio</label>
	        </trigger>
	        <trigger ref="viewattached">
	            <label>View Attached Files</label>
	        </trigger>
	        <group ref="container2" faims_style="orientation">
	          <label></label>
	          <group ref="child1" faims_style="even">
	            <label></label>
	            <trigger ref="save">
	              <label>Save</label>
	            </trigger>
	          </group>
	          <group ref="child2" faims_style="even">
	            <label></label>
	            <trigger ref="clear">
	              <label>Clear</label>
	            </trigger>
	          </group>
	        </group>
	        <trigger ref="delete">
	          <label>Delete</label>
	        </trigger>
	        <trigger ref="close">
	          <label>Close</label>
	        </trigger>
      </group>
      <group ref="tab2" faims_hidden="false">
        <label>Load Entity</label>
       	<select1 ref="entities" faims_certainty="false">
            <label>Entity:</label>
            <item>
            	<label>dummy</label>
            	<value>dummy</value>
          	</item>
        </select1>
        <trigger ref="load">
            <label>Load</label>
        </trigger>
      </group>
    </group>
    <group ref="tabgroup2" faims_rel_type="AboveBelow">
      <label>Relationship</label>
      <group ref="tab1" faims_hidden="false">
        <label>Save Relationship</label>
	        <input ref="name" faims_attribute_name="name" faims_attribute_type="freetext">
	          	<label>Name:</label>
	        </input>
	        <input ref="value" faims_attribute_name="value" faims_attribute_type="freetext">
	            <label>Value:</label>
	        </input>
	        <input ref="timestamp" faims_attribute_name="timestamp" faims_attribute_type="freetext" faims_read_only="true" faims_certainty="false">
              <label>Timestamp:</label>
            </input>
            <select ref="location" faims_attribute_name="location" faims_attribute_type="vocab">
                <label>Location:</label>
                <item>
                	<label>dummy</label>
                	<value>dummy</value>
              	</item>
            </select>
	        <select1 ref="type" faims_attribute_name="type" faims_attribute_type="vocab">
	            <label>Type:</label>
	            <item>
	            	<label>dummy</label>
	            	<value>dummy</value>
	          	</item>
	        </select1>
	        <select1 ref="gallery" type="image" faims_attribute_name="gallery" faims_attribute_type="vocab">
	            <label>Gallery:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select1>
	        <select1 ref="rocks" type="image" faims_attribute_name="rocks" faims_attribute_type="vocab">
	            <label>Rocks:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select1>
	        <select ref="files" type="file" faims_attribute_name="file" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Files:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachfile">
	            <label>Attached File:</label>
	        </trigger>
	        <select ref="pictures" type="camera" faims_sync="true" faims_attribute_name="picture" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Pictures:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachpicture">
	            <label>Take picture</label>
	        </trigger>
	        <select ref="videos" type="video" faims_attribute_name="video" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Videos:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachvideo">
	            <label>Take video</label>
	        </trigger>
	        <select ref="audios" type="file" faims_attribute_name="audio" faims_attribute_type="freetext" faims_annotation="false" faims_certainty="false">
	            <label>Audios:</label>
	            <item>
	                <label>dummy</label>
	                <value>dummy</value>
	            </item>
	        </select>
	        <trigger ref="attachaudio">
	            <label>Record Audio</label>
	        </trigger>
	        <trigger ref="viewattached">
	            <label>View Attached Files</label>
	        </trigger>
	        <group ref="container2" faims_style="orientation">
	          <label></label>
	          <group ref="child1" faims_style="even">
	            <label></label>
	            <trigger ref="save">
	              <label>Save</label>
	            </trigger>
	          </group>
	          <group ref="child2" faims_style="even">
	            <label></label>
	            <trigger ref="clear">
	              <label>Clear</label>
	            </trigger>
	          </group>
	        </group>
	        <trigger ref="delete">
	          <label>Delete</label>
	        </trigger>
	        <trigger ref="close">
	          <label>Close</label>
	        </trigger>
      </group>
      <group ref="tab2">
        <label>Load Relationship</label>
        <select1 ref="relationships" faims_certainty="false">
            <label>Relationship:</label>
            <item>
            	<label>dummy</label>
            	<value>dummy</value>
          	</item>
        </select1>
        <trigger ref="load">
            <label>Load</label>
        </trigger>
      </group>
    </group>
    <group ref="tabgroup3">
      <label>Controls</label>
      <group ref="tab1" faims_hidden="false">
          <label>Controls</label>
          <trigger ref="start">
              <label>Enable Sync</label>
          </trigger>
          <trigger ref="stop">
              <label>Disable Sync</label>
          </trigger>
      </group>
    </group>
    <group ref="tabgroup4">
        <label>Associations</label>
        <group ref="tab1">
            <label>Add Entity Member</label>
            <select1 ref="entities">
                <label>Entities</label>
                <item>
	            	<label>dummy</label>
	            	<value>dummy</value>
	          	</item>
            </select1>
            <select1 ref="relationships">
                <label>Relationships</label>
                <item>
	            	<label>dummy</label>
	            	<value>dummy</value>
	          	</item>
            </select1>
            <input ref="verb">
                <label>Verb</label>
            </input>
            <trigger ref="add">
              <label>Add member</label>
          	</trigger>
        </group>
    </group>
  </h:body>
</h:html>
